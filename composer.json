{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "agencetwogether/filament-optimize-fileupload": "^1.0",
        "asmit/filament-upload": "^2.0",
        "axlon/laravel-postal-code-validation": "^3.8",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-shield": "^4.0",
        "biostate/filament-menu-builder": "^5.0",
        "codeat3/blade-phosphor-icons": "^2.0",
        "diogogpinto/filament-auth-ui-enhancer": "^2.0",
        "filament/filament": "^5.0",
        "filament/spatie-laravel-media-library-plugin": "^5.0",
        "filament/spatie-laravel-settings-plugin": "^5.0",
        "hugomyb/filament-error-mailer": "^5.0",
        "jaocero/radio-deck": "^3.0",
        "joaopaulolndev/filament-pdf-viewer": "^3.0",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.9",
        "laravelcm/laravel-subscriptions": "^1.4",
        "marcelweidum/filament-expiration-notice": "^1.0",
        "ralphjsmit/laravel-filament-seo": "^2.0",
        "rickdbcn/filament-email": "^2.0",
        "shuvroroy/filament-spatie-laravel-backup": "^3.0",
        "spatie/flysystem-dropbox": "^3.0",
        "spatie/laravel-honeypot": "^4.5",
        "stechstudio/filament-impersonate": "^5.0",
        "whitecube/laravel-cookie-consent": "^1.2",
        "yebor974/filament-renew-password": "^3.0",
        "ysfkaya/filament-phone-input": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "fruitcake/laravel-debugbar": "^3.16",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/App.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
